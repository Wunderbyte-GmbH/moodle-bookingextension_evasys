<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/booking/bookingextension/evasys/db" VERSION="2022122000" COMMENT="XMLDB file for Moodle mod/booking"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
<TABLE NAME="bookingextension_evasys" COMMENT="Defines Evasys Data.">
  <FIELDS>
    <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
    <FIELD NAME="optionid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
    <FIELD NAME="formid" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="qrurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="surveyurl" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="timemode" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="starttime" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="endtime" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="durationbeforestart" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="durationafterend" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="organizers" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="notifyparticipants" TYPE="int" LENGTH="1" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="surveyid" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="courseidexternal" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="courseidinternal" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="periods" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
    <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
    <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" SEQUENCE="false"/>
    <FIELD NAME="usermodified" TYPE="int" LENGTH="10" NOTNULL="false" SEQUENCE="false"/>
  </FIELDS>
  <KEYS>
    <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
  </KEYS>
  </TABLE>
  </TABLES>
</XMLDB>
